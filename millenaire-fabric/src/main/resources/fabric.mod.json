{
  "schemaVersion": 1,
  "id": "millenaire",
  "version": "${version}",
  "name": "Millenaire Reborn",
  "description": "An historical village mod for Minecraft 1.21.11 Fabric. Build and interact with historically accurate villages from different cultures.",
  "authors": [
    "Kinniken (Original)",
    "Millenaire Reborn Team"
  ],
  "contact": {
    "homepage": "https://millenaire.org",
    "sources": "https://github.com/millenaire/millenaire-fabric"
  },
  "license": "All Rights Reserved",
  "icon": "assets/millenaire/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.millenaire.MillenaireMod"
    ],
    "client": [
      "org.millenaire.client.MillenaireClientMod"
    ]
  },
  "mixins": [
    "millenaire.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "minecraft": "~1.21.3",
    "java": ">=21",
    "fabric-api": "*"
  }
}
