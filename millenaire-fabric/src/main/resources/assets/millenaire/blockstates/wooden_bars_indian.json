{
    "multipart": [
    	{   "apply": { "model": "millenaire:wooden_bars_indian_post_ends" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "millenaire:wooden_bars_indian_post" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "millenaire:wooden_bars_indian_cap" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "millenaire:wooden_bars_indian_cap", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "millenaire:wooden_bars_indian_cap_alt" }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "millenaire:wooden_bars_indian_cap_alt", "y": 90 }
        },
        {   "when": { "north": true },
            "apply": { "model": "millenaire:wooden_bars_indian_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "millenaire:wooden_bars_indian_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "millenaire:wooden_bars_indian_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "millenaire:wooden_bars_indian_side_alt", "y": 90 }
        }
    ]
}
